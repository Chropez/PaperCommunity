@model PaperCommunity.Models.Game

<fieldset>
    <legend>Game</legend>



   
    <div class="editor-label">
        @Html.LabelFor(model => model.HomePlayer)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.HomePlayer.Username)
        @Html.ValidationMessageFor(model => model.HomePlayer.Username)
    </div>

    <div class="editor-label">
        @Html.LabelFor(model => model.HomeTeam)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.HomeTeam.Name)
        @Html.ValidationMessageFor(model => model.HomeTeam.Name)
    </div>



    <div class="editor-label">
