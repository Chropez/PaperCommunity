    <div class="editor-label">
        @Html.LabelFor(model => model.AwayPlayer)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.AwayPlayer.Username)
        @Html.ValidationMessageFor(model => model.AwayPlayer.Username)
    </div>

    <div class="editor-label">
        @Html.LabelFor(model => model.AwayTeam)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.AwayTeam.Name)
        @Html.ValidationMessageFor(model => model.AwayTeam.Name)
    </div>

    <div class="editor-label">
        @Html.LabelFor(model => model.AwayGoals)
    </div>
    <div class="editor-field">
        @Html.EditorFor(model => model.AwayGoals)
        @Html.ValidationMessageFor(model => model.AwayGoals)
